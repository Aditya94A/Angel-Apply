{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "AngelApply - Apply to jobs on AngelList",
  "icons": {
    "512": "icon-512.png"
  },
  "options_ui": {
    "page": "options/index.html",
    "chrome_style": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://angel.co/*"
      ],
      "js": [
        "options/jquery.min.js",
        "constants.js",
        "content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage"
  ]
}
